{
  "name": "url-minify",
  "version": "2.8.5",
  "description": "Library to shorten url without any api keys",
  "type": "module",
  "packageManager": "pnpm@10.30.3",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/url-minify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/url-minify.git"
  },
  "bugs": "https://github.com/rubiin/url-minify/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && clean-publish",
    "start": "esno src/index.ts",
    "format": "prettier --write --cache \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^28.2.0",
    "@types/node": "^24.11.0",
    "bumpp": "^10.4.1",
    "clean-publish": "^6.0.3",
    "esno": "^4.8.0",
    "oxlint": "^1.50.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "axios": "^1.13.6"
  },
  "clean-publish": {
    "cleanDocs": true,
    "cleanComments": true
  }
}
